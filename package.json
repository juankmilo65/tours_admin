{
  "name": "tours_admin",
  "version": "1.0.0",
  "description": "Tours Admin Dashboard - Modern admin panel for managing tours, reservations, and payments",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc --noEmit",
    "prepare": "husky"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.11.2",
    "@remix-run/node": "^2.17.2",
    "@remix-run/react": "^2.17.2",
    "@remix-run/serve": "^2.17.2",
    "axios": "^1.13.2",
    "dotenv": "^17.2.3",
    "isbot": "^5.1.17",
    "jsonwebtoken": "^9.0.3",
    "jwt-decode": "^4.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.2.0",
    "redux-persist": "^6.0.0",
    "remix-utils": "^7.7.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@remix-run/dev": "^2.17.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/jwt-decode": "^3.1.0",
    "@types/node": "^22.9.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@typescript-eslint/parser": "^8.14.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "postcss": "^8.4.49",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.15",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vite": "^6.0.1",
    "vite-tsconfig-paths": "^5.1.3"
  },
  "engines": {
    "node": ">=20.0.0"
